genisoimage -U -r -v -T -J -joliet-long -V "UUI" -volset "UUI" -A "UUI" -b isolinux/isolinux.bin -c isolinux/boot.cat -no-emul-boot -boot-load-size 4 -boot-info-table -eltorito-alt-boot -e newsdm/images/diskboot.img -no-emul-boot -o ./NEWISO.iso


mkisofs -o ../new.iso -b isolinux/isolinux.bin -c isolinux/boot.cat -no-emul-boot -boot-load-size 4 -boot-info-table -J -r /var/lib/libvirt/images/newsdm

